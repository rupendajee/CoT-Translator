# Ingest
IN_UDP_PORT=5010
# Optional payload prefix filter: if set, packets must start with this exact string
# e.g. IN_PSK="MYSECRET:"
IN_PSK=

# CoT fields
COT_TYPE=a-f-G-U-C
STALE_SECS=120
COT_HOW=m-g

# ---- Direct to TAK TLS Input (recommended) ----
OUT_TAK_TLS_ENABLE=true
TAK_TLS_ADDR=tak.example.org:8089
TAK_CA_CERT=/opt/cot/ca.pem
TAK_CLIENT_CERT=/opt/cot/translator.pem
TAK_CLIENT_KEY=/opt/cot/translator.key
TAK_TLS_INSECURE=false

# ---- Optional additional outputs (usually leave disabled) ----
OUT_TCP_SERVER_ENABLE=false
TCP_LISTEN=:8087

OUT_UDP_ENABLE=false
OUT_UDP_ADDR=239.5.0.1
OUT_UDP_PORT=6969
